# Discord Audit Stream Configuration
# Copy this file to .env or set these environment variables

# Required: Discord Webhook URL for sending audit messages
DISCORD_AUDIT_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Optional: Fallback channel ID if webhook fails (uses openclaw message send)
DISCORD_AUDIT_CHANNEL_ID=1474043146705830112

# Optional: Rate limiting between messages (milliseconds)
# DISCORD_AUDIT_RATE_LIMIT_MS=2000

# Optional: Batch window for grouping events (milliseconds)
# DISCORD_AUDIT_BATCH_WINDOW_MS=8000

# Optional: OpenClaw binary path
# OPENCLAW_BIN=openclaw
